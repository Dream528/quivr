{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Article": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "slug": {
      "type": "string",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
      "required": true,
      "unique": true
    }
  }
}
